xInt = linspace(0, 1, 100); 
yInt = linspace(1, 2, 100); 

xInt = xInt(xInt ~= yInt .^ 2);
[X, Y] = meshgrid(xInt, yInt);

f = @(x, y) sin(x.^2 - y.^4) / (x.^2 - y.^4);

% -------------------- (a) -------------------------------

Z = f(X, Y); 

surf(Z);
title("plot of f")
xlabel("x-axis")
ylabel("y-axis")
zlabel("f(x, y)")



